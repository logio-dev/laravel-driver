{
  "name": "logio-dev/laravel-driver",
  "description": "The official driver for Laravel, send your application logs to Logio.dev.",
  "keywords": [
    "laravel",
    "log",
    "driver",
    "logio"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ben",
      "email": "hello@logio.dev"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "guzzlehttp/guzzle": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "Logio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Logio\\LogioServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/framework": "8.x",
    "phpunit/phpunit": "^9",
    "orchestra/testbench": "^6.19"
  }
}
